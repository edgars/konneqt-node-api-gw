{
  "cors": {
    "enabled": true,
    "origin": "*",
    "methods": ["GET", "POST", "PUT", "DELETE"]
  },
  "routes": [
    {
      "method": "GET",
      "url": "/api/users",
      "proxy": {
        "target": "https://jsonplaceholder.typicode.com/users"
      },
      "rateLimit": {
        "max": 50,
        "timeWindow": "1 minute"
      }
    },
    {
      "method": "POST",
      "url": "/api/posts",
      "proxy": {
        "target": "https://jsonplaceholder.typicode.com/posts"
      },
      "rateLimit": {
        "max": 20,
        "timeWindow": "1 minute"
      },
      "cors": false
    }
  ]
}
